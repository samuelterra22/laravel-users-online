{
    "name": "samuelterra22/laravel-users-online",
    "description": "This package will provide an online users management.",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": [
        "laravel",
        "users",
        "online",
        "management",
        "package"
    ],
    "authors": [
        {
            "name": "Samuel Terra",
            "email": "samuelterra22@gmail.com",
            "homepage": "https://samuelterra.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.4",
        "php-coveralls/php-coveralls": "^2.5",
        "illuminate/database": "^12.20.0",
        "illuminate/cache": "^12.20.0",
        "illuminate/support": "^12.20.0",
        "illuminate/auth": "^12.20.0",
        "illuminate/session": "^12.20.0"
    },
    "autoload": {
        "psr-4": {
            "SamuelTerra22\\UsersOnline\\": "src/",
            "SamuelTerra22\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SamuelTerra22\\UsersOnline\\Providers\\UsersOnlineEventServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "vildanbina/composer-upgrader": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
